{
  "name": "yesfundme",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=@yesfundme/client\" \"npm run dev --workspace=@yesfundme/server\"",
    "seed": "npm run seed --workspace=@yesfundme/database",
    "reset-db": "rm -f packages/server/yesfundme.db && npm run seed"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
